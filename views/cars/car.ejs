                    <h1><span>Car</span> Rental</h1>

                </div>
                <div class="browser-button">
                    <button class="tab a active">View all</button>
                    <button class="tab">Gambia</button>
                    <button class="tab">Senegal</button>
                </div>
            </div>
            <div class="tabs">
                <button class="tab">SUVs</button>
                <button class="tab">Sedan</button>
                <button class="tab">Sports</button>
                <button class="tab">Adventure</button>
            </div>
        </div>
        <div id="car-rent" class="container swiper-container">
            <div class="cards swiper-wrapper">                
                <% cars.forEach(car => { %>
                    <a href="/cars/<%= car._id %>" class="card swiper-slide">
                        <div class="image carousel-images">
                            <!-- Display up to 4 images in the carousel -->
                                <% car.images.slice(0, 4).forEach((image, index) => { %>
                                    <img src="<%= image.url %>" 
                                         alt="<%= car.name %>" 
                                         class="carousel-img" 
                                         data-index="<%= index %>" 
                                         style="<%= index !== 0 ? 'display:none;' : '' %>">
                                <% }) %>
                            <div class="badge special">SPECIAL</div>
                        </div>
                
                        <!-- Dynamic Pagination Lines -->
                        <div class="pagination-container">
                            <% car.images.slice(0, 4).forEach((image, index) => { %>
                                <span class="pagination-line <%= index === 0 ? 'active' : '' %>" 
                                      data-target="<%= index %>"></span>
                            <% }) %>
                        </div>
                
                        <h3 class="font-semibold"><%= car.name %></h3>
                        <p class="price">
                            <% if (car.price.original > car.price.msrp) { %>
                            <span class="original-price">$<%= car.price.original.toLocaleString() %></span>
                            <% } %> 
                            $<%= car.price.msrp.toLocaleString() %>
                        </p>
                        <div class="details">
                            <span><%= car.transmission %> <i class="ri-settings-line"></i></span>
                            <span><%= car.fuelType %> <i class="ri-dashboard-line"></i></span>
                            <span><%= car.mileage %> mi <i class="ri-map-pin-line"></i></span>
                        </div>
                    </a>
                <% }) %>
                
                
