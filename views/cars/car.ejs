<body>

    <div class="relative hero-container">
        <!-- Image Background -->
        <img src="https://img.freepik.com/premium-photo/futuristic-supercar-concept_849715-16503.jpg?w=826"
            alt="Hero Image" class="absolute inset-0 w-full h-full object-cover">
        <!-- Overlay (optional) -->
        <div class="absolute inset-0 bg-black bg-opacity-50"></div>


        <h1 style="white-space: nowrap;" class="hero-title">15 000+</h1>
        <p class="hero-span font-semibold">Vehicle Available</p>
    </div>

    <div class="all-outer">
        <!-- Category Section -->
        <div class="rental-category-outer">
            <div class="rental-category">
                <!-- Add A Car Category -->
                <div class="category-box category-1" data-target="form-add-car">
                    <span class="category-icon"><i class="ri-add-line"></i></span>
                    <span class="category-text pick font-medium">Add A Car</span>
                </div>
                <!-- Pickup & Dropoff Category -->
                <div class="category-box category-2" data-target="form-pickup-dropoff">
                    <span class="category-icon"><i class="ri-send-plane-line"></i></span>
                    <span class="category-text pick font-medium">Pickup & Dropoff</span>
                </div>
                <!-- Rent A Car Category -->
                <div class="category-box" data-target="form-rent-car">
                    <span class="category-icon"><i class="ri-taxi-line"></i></span>
                    <span class="category-text rent font-medium">Rent A Car</span>
                </div>
            </div>
        </div>
        <!-- Forms Section -->
        <div class="rental-forms">
            <!-- Form for Add A Car -->
            <div id="form-add-car" class="form-container">
                <form>
                    <div class="form-row">
                        <input type="text" id="car-make" placeholder="Enter car make">


                        <input type="text" id="car-model" placeholder="Enter car model">


                        <input type="number" id="car-price" placeholder="Enter price">


                        <input type="text" id="textInput" name="textInput" placeholder="Enter A Location">
                        <!-- Green Button with Search Icon and Text -->
                        <button type="button" class="search-button">
                            <span class="search-icon"><i class="ri-stack-fill"></i></span> Add Car
                        </button>
                    </div>
                </form>
            </div>


            <!-- Form for Pickup & Dropoff -->
            <div id="form-pickup-dropoff" class="form-container hidden">
                <form>
                    <div class="form-row">
                        <input type="text" id="pickup-location" placeholder="Enter pickup location">


                        <input type="text" id="dropoff-location" placeholder="Enter dropoff location">


                        <input type="date" id="pickup-date">


                        <input type="date" id="dropoff-date">


                        <input type="text" id="textInput" name="textInput" placeholder="Enter A Location">
                        <!-- Green Button with Search Icon and Text -->
                        <button type="button" class="search-button">
                            <span class="search-icon"><i class="ri-taxi-fill"></i></span> Order
                        </button>
                    </div>
                </form>
            </div>


            <!-- Form for Rent A Car -->
            <div id="form-rent-car" class="form-container hidden">
                <form>
                    <div class="form-row">
                        <select id="rental-make">
                            <option value="">Rental Make</option>
                            <option value="Toyota">Toyota</option>
                            <option value="Honda">Honda</option>
                            <option value="Ford">Ford</option>
                        </select>


                        <select id="rental-model">
                            <option value="">Rental Model</option>
                            <option value="Corolla">Corolla</option>
                            <option value="Civic">Civic</option>
                            <option value="Mustang">Mustang</option>
                        </select>


                        <input type="number" id="max-price" placeholder="Enter max price">


                        <input type="text" id="textInput" name="textInput" placeholder="Enter A Location">
                        <!-- Green Button with Search Icon and Text -->
                        <button type="button" class="search-button">
                            <span class="search-icon"><i class="ri-search-line"></i></span> 31 cars
                        </button>
                    </div>
                </form>
            </div>
        </div>
